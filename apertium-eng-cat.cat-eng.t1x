    <def-macro n="firstWord" npar="1" comment="Defineix les majúscules/minúscules del chunk a partir d'un element">
      <choose>
        <when>
          <test>
            <equal>
              <var n="EOS"/>
              <lit v="true"/>
            </equal>
          </test>
          <choose>
            <when>
              <test>
                <not>
                  <equal>
                    <clip pos="1" side="tl" part="a_np_acr"/>
                    <lit v=""/>
                  </equal>
                </not>
              </test>
              <let>
                <var n="caseFirstWord"/>
                <lit v="aa"/>
              </let>
            </when>
            <when>
              <test>
                <or>
                  <equal>
                    <case-of pos="1" side="tl" part="lemh"/>
                    <lit v="AA"/>
                  </equal>
                  <and>
                    <equal>
                      <case-of pos="1" side="sl" part="lemh"/>
                      <lit v="Aa"/>
                    </equal>
                    <equal>
                      <case-of pos="1" side="tl" part="lemh"/>
                      <lit v="aa"/>
                    </equal>
                  </and>
                  <and>
                    <equal>
                      <case-of pos="1" side="tl" part="lemh"/>
                      <lit v="Aa"/>
                    </equal>
                    <not>
                      <equal>
                        <clip pos="1" side="tl" part="a_np_acr"/>
                        <lit v=""/>
                      </equal>
                    </not>
                  </and>
                  <!-- Elements amb majúscula forçada al bidix -->
                  <equal>
                    <clip pos="1" side="tl" part="a_maj"/>
                    <lit-tag v="maj"/>
                  </equal>
                </or>
              </test>
              <let>
                <var n="caseFirstWord"/>
                <lit v="Aa"/>
              </let>
            </when>
            <otherwise>
              <let>
                <var n="caseFirstWord"/>
                <case-of pos="1" side="tl" part="lemh"/>
              </let>
              <modify-case>
                <clip pos="1" side="tl" part="lemh"/>
                <lit v="aa"/>
              </modify-case>
            </otherwise>
          </choose>
        </when>
        <otherwise>
          <let>
            <var n="caseFirstWord"/>
            <lit v="aa"/>
          </let>
          <choose>
            <!-- Elements amb minúscula forçada al bidix -->
            <when>
              <test>
                <equal>
                  <clip pos="1" side="tl" part="a_maj"/>
                  <lit-tag v="min"/>
                </equal>
              </test>
              <modify-case>
                <clip pos="1" side="tl" part="lemh"/>
                <lit v="aa"/>
              </modify-case>
            </when>
          </choose>
        </otherwise>
      </choose>
      <let>
        <var n="EOS"/>
        <lit v="false"/>
      </let>
    </def-macro>



