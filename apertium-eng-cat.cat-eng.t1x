		  <tag><var n="SVnegatiu"/></tag>
		  <tag><clip pos="4" side="tl" part="a_verb"/></tag>
		  <tag><var n="temps_chunk"/></tag>
		  <tag><var n="pers_chunk"/></tag>
		  <tag><var n="nbr_chunk"/></tag>
		</tags>
		<lu>
		  <get-case-from pos="2">
