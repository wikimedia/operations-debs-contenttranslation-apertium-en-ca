 <rule comment="REGLA: PROCLÍTIC ESTAR GER (els estan buscant) mireia falta els negatius pingui i amb dos proclítics">
      <pattern>
	<pattern-item n="pro"/>
	<pattern-item n="estar"/>
	<pattern-item n="ger"/>
      </pattern>
      <action>
	<call-macro n="firstWord">
	  <with-param pos="1"/>
	</call-macro>
