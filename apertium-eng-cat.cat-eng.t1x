		  <clip pos="3" side="tl" part="sep"/>
		  <clip pos="3" side="tl" part="temps"/>
		  <clip pos="3" side="tl" part="lemq"/>
		</lu>
	      </chunk>
	    </out>


	    <choose><!--després d'imprimir el verb, mirem el proclític -->
	      <when>
		<test>
		  <and>
		    <equal>
		      <clip pos="3" side="sl" part="pron"/>
		      <lit-tag v="pron"/>
		    </equal>
		    <equal>
		      <clip pos="1" side="sl" part="pers"/>
		      <clip pos="2" side="sl" part="pers"/>
		    </equal>
		    <or>
		      <equal>
			<clip pos="1" side="sl" part="nbr"/>
			<clip pos="2" side="sl" part="nbr"/>
		      </equal>
		      <equal caseless="yes">
			<clip pos="1" side="sl" part="lem"/>
			<lit v="es"/><!--és 'sp' -->
		      </equal>
		    </or>
		  </and>
		</test>
		<call-macro n="f_bcond"><!--imprimeix el blanc si té info de format, el pronom no l'imprimeixis -->
		  <with-param pos="2"/>
		  <with-param pos="3"/>
		</call-macro>
	      </when>
	      <otherwise>
		<choose>
		  <when>
		    <test>
		      <equal caseless="yes">
			<clip pos="1" side="sl" part="lem"/>
			<lit v="es"/>
		      </equal>
		    </test>
		    <let>
		      <var n="lema"/>
		      <lit v="ref"/>
		    </let>
		    <let>
		      <var n="nom_chunk"/>
		      <lit-tag v="ref"/>
		    </let>
		  </when>
		  <otherwise>
		    <let>
		      <var n="lema"/>
