		  <clip pos="3" side="sl" part="nbr"/>
		  <lit-tag v="sg"/>
		</equal>
	      </and>
	    </test>
	    <let>
	      <clip pos="4" side="tl" part="lem"/>
	      <get-case-from pos="4">
		<lit v="be"/>
	      </get-case-from>
	    </let>
	    <let>
	      <clip pos="4" side="tl" part="a_verb"/>
