			<lit v="es"/><!--és 'sp' -->
		      </equal>
		    </or>
		  </and>
		</test>
		<call-macro n="f_bcond"><!--imprimeix el blanc si té info de format, el pronom no l'imprimeixis -->
		  <with-param pos="2"/>
	          <with-param pos="3"/>
		</call-macro>
	      </when>
	      <otherwise>
		<choose>
		  <when>
		    <test>
		      <equal caseless="yes">
			<clip pos="1" side="sl" part="lem"/>
			<lit v="es"/>
		      </equal>
